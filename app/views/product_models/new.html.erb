<h1>Cadastrar Novo Modelo de Produto</h1>

<%= form_with(model: @product_model) do |f|%>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :weight %>
    <%= f.number_field :weight %>
  </div>
  <div class="field">
    <%= f.label :height %>
    <%= f.number_field :height %>
  </div>
  <div class="field">
    <%= f.label :width %>
    <%= f.number_field :width %>
  </div>
  <div class="field">
    <%= f.label :length %>
    <%= f.number_field :length %>
  </div>
  <div class="field">
    <%= f.label :sku %>
    <%= f.text_field :sku %>
  </div>

  <div>
    <%= f.label :supplier_id %>
    <%= f.collection_select :supplier_id, @suppliers, :id, :brand_name %>


  </div>





  <div class="actions">
    <%= f.submit "Enviar"%>
  </div>



<% end %>